{
  "name": "zindexer",
  "description": "Fetches data from GitHub, Jira and pushes it to Elasticsearch",
  "version": "0.0.1",
  "author": "Francois Gerthoffert @Fgerthoffert",
  "bin": {
    "zindexer": "./bin/run"
  },
  "bugs": "https://github.com/zencrepes/zindexer/issues",
  "dependencies": {
    "@elastic/elasticsearch": "^7.5.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/js-yaml": "^3.12.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "js-yaml": "^3.13.1",
    "load-yaml-file": "^0.2.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "tslib": "^1",
    "uuid-by-string": "^3.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^12.12.17",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "chai": "^4",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^13",
    "prettier": "^1.19.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/zencrepes/zindexer",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "zindexer",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "zencrepes/zindexer",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint . --ext .js,.ts"
  },
  "types": "lib/index.d.ts"
}
